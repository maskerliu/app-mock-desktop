{
  "name": "app-mock-desktop",
  "version": "0.0.5",
  "author": "liuzhufeng",
  "description": "Application network mock & proxy project",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true node --max_old_space_size=9000 .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "publish": "build --mac -p always"
  },
  "build": {
    "productName": "AppMock",
    "appId": "com.lynxchina.desktop.appmock",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "maskerliu",
        "repo": "app-mock-desktop",
        "releaseType": "pre-release",
        "releaseNotes": "1、新增了本地服务端口自定义支持；\n2、对请求响应内容中的图片、音频加入预览支持；\n3、新增打点数据预览；\n4、增加功能Hand Book页；\n"
      }
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "13.7.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/body-parser": "1.19.0",
    "MD5": "^1.3.0",
    "adm-zip": "^0.4.13",
    "ajv": "^6.5.0",
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "better-scroll": "^1.15.2",
    "body-parser": "^1.19.0",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "class-transformer": "^0.2.3",
    "copy-webpack-plugin": "^4.5.1",
    "cors": "^2.8.5",
    "cross-env": "^5.1.6",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "dexie": "^2.0.4",
    "electron": "^5.0.11",
    "electron-builder": "^20.44.4",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.4",
    "element-ui": "^2.12.0",
    "express": "^4.17.1",
    "file-loader": "^1.1.11",
    "flv.js": "^1.5.0",
    "grapher-json-ui": "^1.0.4",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "0.4.0",
    "moment": "^2.24.0",
    "mousetrap": "^1.6.3",
    "multispinner": "^0.2.1",
