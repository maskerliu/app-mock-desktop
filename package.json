    "vue-template-compiler": "^2.5.16",
    "vuex": "^3.1.0",
    "vuex-class": "^0.3.2",
    "vuex-electron": "^1.0.0",
    "v-jsoneditor": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.9",
