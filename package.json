{
  "name": "app-mock-desktop",
  "version": "1.0.0",
  "author": "liuzhufeng",
  "description": "Application network mock & proxy project",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true node --max_old_space_size=9000 .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "publish": "build --mac -p always"
  },
  "build": {
    "productName": "AppMock",
    "appId": "com.lynxchina.desktop.appmock",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "pouchdb": "^7.2.1",
    "pouchdb-find": "^7.2.1"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "nodejs-websocket": "^1.7.2",
    "adm-zip": "^0.4.13",
    "json-bigint": "^0.3.0",
    "@types/node": "13.7.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/body-parser": "1.19.0",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-find": "^6.3.5",
    "md5": "^2.2.1",
    "ajv": "^6.12.0",
    "animate.css": "^3.7.2",
    "better-scroll": "^1.15.2",
    "body-parser": "^1.19.0",
    "cfonts": "^2.8.0",
    "chalk": "^2.4.1",
    "class-transformer": "^0.2.3",
    "copy-webpack-plugin": "^4.5.1",
    "cors": "^2.8.5",
    "cross-env": "^5.1.6",
    "crypto-js": "^3.1.9-1",
    "css-loader": "^3.4.2",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^8.1.0",
    "electron-builder": "^22.4.1",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "element-ui": "^2.12.0",
    "express": "^4.17.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.1",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "0.9.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "qrcode.vue": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "svgaplayerweb": "^2.3.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
    "url-loader": "^3.0.0",
    "vue": "^2.6.11",
    "vue-awesome-swiper": "^3.1.3",
    "vue-class-component": "^7.2.3",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.0",
    "vue-native-websocket": "2.0.14",
    "vue-property-decorator": "^8.4.0",
    "vue-router": "^3.1.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.3",
    "vuex-class": "^0.3.2",
    "v-jsoneditor": "^1.2.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  }
}
