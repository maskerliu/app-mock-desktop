{
  "name": "app-mock-desktop",
  "version": "1.0.3",
  "author": "liuzhufeng",
  "description": "Application network mock & proxy project",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "cross-env ELECTRON_DISABLE_SECURITY_WARNINGS=true node --max_old_space_size=9000 .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "dll": "webpack  --mode production  --profile --inline --config .electron-vue/webpack.dll.config.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "publish": "build --mac -p always",
    "asar": "node .electron-vue/compress-asar.js"
  },
  "build": {
    "productName": "AppMock",
    "appId": "com.lynxchina.desktop.appmock",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/*/**/*",
      {
        "from": "node_modules/electron-asar-hot-updater/",
        "to": "./node_modules/electron-asar-hot-updater",
        "filter": [
          "**/*"
        ]
      }
    ],
    "extraResources": [],
    "pkg": {
      "installLocation": "/Applications"
    },
    "mac": {
      "icon": "build/icons/icon.icns",
      "target": [
        "pkg"
      ]
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "json-bigint": "^0.3.0",
    "cors": "^2.8.5",
    "sockjs": "^0.3.21",
    "axios": "^0.19.0",
    "protobufjs": "^6.8.9",
    "pouchdb": "^7.2.1",
    "pouchdb-find": "^7.2.1"
  },
  "devDependencies": {
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-find": "^6.3.5",
    "@types/body-parser": "1.19.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.2",
    "@types/cors": "^2.8.7",
    "@types/node": "13.7.2",
    "adm-zip": "^0.4.13",
    "ajv": "^6.12.0",
    "async-validator": "^3.3.0",
    "babel-loader": "^7.1.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-component": "^1.1.1",
    "babel-preset-es2015": "^6.24.1",
    "camel-2-dash": "^0.1.0",
    "cfonts": "^2.8.0",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "9.2.1",
    "electron-builder": "^22.6.1",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.0.0",
    "element-ui": "^2.13.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "md5": "^2.2.1",
    "mini-css-extract-plugin": "0.9.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.13.1",
    "sockjs-client": "^1.5.0",
    "page-skeleton-webpack-plugin": "^0.10.12",
    "qrcode.vue": "^1.7.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "ts-import-plugin": "^1.6.6",
    "typescript": "^3.9.3",
    "url-loader": "^3.0.0",
    "v-jsoneditor": "^1.4.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.2",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vue-virtual-scroll-list": "^2.3.0",
    "vuex": "^3.4.0",
    "vuex-class": "^0.3.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}